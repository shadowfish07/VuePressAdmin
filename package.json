    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.0.0",
    "apidoc": "^0.51.1",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "chance": "^1.1.8",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^5.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
